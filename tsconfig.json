{
  "extends": "@tsconfig/node16/tsconfig.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "outDir": "./dist",
    "skipLibCheck": false,
    "sourceMap": true,
    "strict": true,
    "target": "ES2021",
    "noUncheckedIndexedAccess": true
  }
}
